+            "place_detail/{placeId}",
+            arguments = listOf(navArgument("placeId") { type = NavType.IntType })
+        ) { backStackEntry ->
+            val placeId = backStackEntry.arguments?.getInt("placeId") ?: 0
+            PlaceDetailScreen(navController, placeId)
+        }
+    }
+}
